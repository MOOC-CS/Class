library(rattle)
library(caret)
library(randomForest)

training <- read.csv("pml-training.csv")
testing <- read.csv("pml-testing.csv")

Train <- createDataPartition(final_data$classe,  p=0.70, list=FALSE)
Traingset <- final_data[Train,]
testingset <- final_data[-Train,]



model <- train(classe ~ ., data=Traingset, model="rf", trControl=tr)
predictions <- predict(model, newdata=testing)

confusionMatrix(testingset$classe, predictions)


